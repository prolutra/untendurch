{
  "name": "untendurch",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "AGPL-3.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "self-update": "yarn set version stable && yarn install",
    "up": "yarn upgrade-interactive",
    "build-frontend": "yarn workspace @untendurch/frontend run build",
    "check": "yarn workspaces foreach -A run check",
    "db:download": "yarn workspace @untendurch/tools run db:download",
    "db:import": "yarn workspace @untendurch/tools run db:import",
    "db:sync": "yarn workspace @untendurch/tools run db:sync",
    "files:download": "yarn workspace @untendurch/tools run files:download",
    "files:import": "yarn workspace @untendurch/tools run files:import",
    "files:sync": "yarn workspace @untendurch/tools run files:sync",
    "sync": "yarn workspace @untendurch/tools run sync"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "eslint": "^9.28.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^16.2.0",
    "typescript-eslint": "^8.33.0"
  },
  "packageManager": "yarn@4.12.0"
}
