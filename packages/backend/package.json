{
  "name": "@untendurch/backend",
  "packageManager": "yarn@4.2.2",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "dotenv -e .env node ./dist/server.js",
    "dev": "tsc -w & dotenv -e .env nodemon ./dist/server.js",
    "build": "tsc",
    "lint": "eslint ./src/**/*.{js,ts,jsx,tsx,html}",
    "db:start": "docker compose -f docker-compose.dev.yml up -d",
    "db:stop": "docker compose -f docker-compose.dev.yml down",
    "db:clean": "docker volume rm backend_mongodb_data"
  },
  "dependencies": {
    "@parse/fs-files-adapter": "^3.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "lodash-es": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "parse": "^5.1.0",
    "parse-dashboard": "^5.4.0",
    "parse-server": "^7.0.0",
    "sharp": "^0.33.4",
    "uuid": "^9.0.1",
    "write-excel-file": "^2.0.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/cors": "^2.8.17",
    "@types/lodash-es": "^4.17.12",
    "@types/multer": "^1.4.11",
    "@types/parse": "^3.0.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.0",
    "typescript": "^5.4.5"
  }
}
