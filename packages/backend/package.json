{
  "name": "@untendurch/backend",
  "packageManager": "yarn@4.2.2",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "dotenv -e .env node ./dist/server.js",
    "dev": "tsc -w & dotenv -e .env nodemon ./dist/server.js",
    "build": "tsc",
    "check": "tsc --noEmit && eslint . --fix && prettier --write --log-level warn \"src/**/*.{ts,js,json}\"",
    "lint": "eslint .",
    "format": "prettier --write --log-level warn \"src/**/*.{ts,js,json}\"",
    "db:start": "docker compose -f docker-compose.dev.yml up -d",
    "db:stop": "docker compose -f docker-compose.dev.yml down",
    "db:clean": "docker volume rm backend_untendurch_mongodb_data"
  },
  "dependencies": {
    "@parse/fs-files-adapter": "^3.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express": "5.2.0",
    "multer": "^1.4.5-lts.2",
    "parse": "6.1.1",
    "parse-dashboard": "8.1.0",
    "parse-server": "8.4.0",
    "sharp": "^0.33.5",
    "uuid": "^10.0.0",
    "write-excel-file": "^2.3.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/cors": "^2.8.19",
    "@types/multer": "^1.4.13",
    "@types/parse": "^3.0.9",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.17.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "globals": "^15.14.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.4.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.18.1"
  }
}
