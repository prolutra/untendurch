{
  "name": "@untendurch/frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "AGPL-3.0",
  "dependencies": {
    "@mdx-js/react": "^3.0.1",
    "lodash-es": "^4.17.21",
    "mobx": "6.12.3",
    "mobx-keystone": "^1.10.1",
    "mobx-react-lite": "^4.0.7",
    "ol": "9.2.4",
    "parse": "^5.1.0",
    "polished": "^4.3.1",
    "proj4": "^2.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "react-router-dom": "^6.23.1",
    "tslog": "^4.9.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode development",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode prod",
    "ix-de": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/de.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "ix-en": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "ix-fr": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/fr.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "ix-it": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/it.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n-extract": "yarn ix-de && yarn ix-en && yarn ix-fr && yarn ix-it",
    "ic-de": "formatjs compile lang/de.json --ast --out-file ./src/compiled-lang/de.json",
    "ic-en": "formatjs compile lang/en.json --ast --out-file ./src/compiled-lang/en.json",
    "ic-fr": "formatjs compile lang/fr.json --ast --out-file ./src/compiled-lang/fr.json",
    "ic-it": "formatjs compile lang/it.json --ast --out-file ./src/compiled-lang/it.json",
    "i18n-compile": "yarn ic-de && yarn ic-en && yarn ic-fr && yarn ic-it",
    "pretty": "prettier --write \"{src,public}/**/*.{ts,tsx,scss,json,html}\"",
    "lint": "eslint ./src/**/*.{js,ts,jsx,tsx,html}",
    "up": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.24.6",
    "@babel/plugin-transform-class-properties": "^7.24.6",
    "@formatjs/cli": "^6.2.12",
    "@types/lodash-es": "^4.17.12",
    "@types/parse": "^3.0.9",
    "@types/proj4": "^2.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "babel-plugin-formatjs": "^10.5.16",
    "csv-parser": "^3.0.0",
    "daisyui": "^4.11.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "postcss": "^8.4.38",
    "prettier": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3",
    "vite": "^5.2.12",
    "vite-bundle-analyzer": "^0.9.4",
    "vite-plugin-checker": "^0.6.4",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "packageManager": "yarn@4.2.2"
}
